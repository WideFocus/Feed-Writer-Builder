{
    "name": "widefocus/feed-writer-builder",
    "description": "This package contains models to build a feed writer based on a feed entity.",
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Ashoka de Wit",
            "email": "ashoka@widefocus.net"
        }
    ],
    "require": {
        "php": "^7.0",
        "widefocus/feed-entity": "^1.0",
        "widefocus/feed-writer": "^1.0",
        "widefocus/filter": "^1.0",
        "widefocus/parameters": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "WideFocus\\Feed\\Writer\\Builder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WideFocus\\Feed\\Writer\\Builder\\Tests\\": "tests"
        }
    }
